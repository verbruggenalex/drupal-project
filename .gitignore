# Ignore directories generated by Composer
/vendor/
/build/sites/all/drush/
/build/sites/all/modules/contrib/
/build/sites/all/themes/contrib/
/build/sites/all/libraries/

# Ignore Drupal 7 core files.
/build/*
!/build/*/
/build/includes/
/build/misc/
/build/modules/
/build/profiles/README.txt
/build/profiles/minimal/
/build/profiles/standard/
/build/profiles/testing/
/build/scripts/
/build/sites/README.txt
/build/sites/all/*/README.txt
/build/sites/example.sites.php
/build/themes/

# Ignore configuration files that may contain sensitive information.
/build/sites/*/*.settings.php
/build/sites/*/settings*.php

# Ignore paths that contain user-generated content.
/build/sites/*/files
/build/sites/*/private

# Ignore files generated by PhpStorm
/.idea/
